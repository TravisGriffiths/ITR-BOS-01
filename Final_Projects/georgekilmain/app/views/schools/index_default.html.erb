<h1>Listing schools</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Wins</th>
    <th>Losses</th>
    <th>Gmaps</th>
    <th>League</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @schools.each do |school| %>
  <tr>
    <td><%= school.name %></td>
    <td><%= school.latitude %></td>
    <td><%= school.longitude %></td>
    <td><%= school.wins %></td>
    <td><%= school.losses %></td>
    <td><%= school.gmaps %></td>
    <td><%= school.league %></td>
    <td><%= school.content %></td>
    <td><%= link_to 'Show', school %></td>
    <td><%= link_to 'Edit', edit_school_path(school) %></td>
    <td><%= link_to 'Destroy', school, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New School', new_school_path %>
